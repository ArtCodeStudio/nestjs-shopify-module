    "@types/jest": "^26.0.13",
    "@types/mongodb": "^3.5.27",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.10.0",
    "@types/node-fetch": "^2.5.7",
    "@types/p-queue": "^3.2.1",
    "@types/p-retry": "^3.0.1",
    "@types/passport": "^1.0.4",
    "@types/retry": "^0.12.0",
    "@types/socket.io": "^2.1.11",
    "@types/supertest": "^2.0.10",
    "jest": "^26.4.2",

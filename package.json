    "@nestjs/common": "^5.6.2",
    "@nestjs/core": "^5.6.2",
    "@nestjs/testing": "^5.6.2",
    "@types/cache-manager": "^1.2.6",
    "@types/deepmerge": "^2.2.0",
    "@types/elasticsearch": "^5.0.30",
    "@types/express": "^4.17.7",
    "@types/express-session": "^1.17.0",
    "@types/express-socket.io-session": "^1.3.2",
    "@types/jest": "^23.3.13",

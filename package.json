    "passport-shopify": "^0.1.2",
    "shopify-admin-api": "^2.15.0-alpha.5",
    "shopify-token": "^3.0.3",
    "socket.io-redis": "^5.2.0",
    "swiftype-app-search-node": "^0.3.1"
  },

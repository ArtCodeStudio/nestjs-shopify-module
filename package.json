{
  "name": "nest-shopify",
  "version": "7.5.5",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/index.d.ts",
  "description": "Nest module for Shopify app backend",
  "author": "Moritz Raguschat<moritz@artandcode.studio>, Pascal Garber<pascal@artandcode.studio>",
  "license": "MIT",
  "url": "https://github.com/ArtCodeStudio/nestjs-shopify-module",
  "repository": {
    "type": "git",
    "url": "git@github.com:ArtCodeStudio/nestjs-shopify-module.git"
  },
  "scripts": {
    "b": "tsc src/p-map.ts",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "watch": "npm run build -- --watch",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --ignore-pattern '*.spec.ts' --fix",
    "link": "npm link && npm install && npm link shopify-admin-api && cd node_modules/shopify-admin-api && npm install",
    "test": "pnpify jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.config.js",
    "test:coverage": "jest --config=jest.config.js --coverage --coverageDirectory=coverage",
    "test:product": "SWIFTYPE=1 MONGODB=1 jest --config=jest.config.js 'src/.*products.*\\.spec.ts$'",
    "test:product:watch": "SWIFTYPE=1 MONGODB=1 jest --watch --config=jest.config.js 'src/.*products.*\\.spec.ts$'",
    "test:sync": "SWIFTYPE=1 MONGODB=1 jest --config=jest.config.js 'src/sync.*\\.spec.ts$'",
    "test:sync:watch": "SWIFTYPE=1 MONGODB=1 jest --watch --config=jest.config.js 'src/sync.*\\.spec.ts$'"
  },
  "files": [
    "src/",
    "dist/"
  ],
  "dependencies": {
    "@nestjs/common": "^7.5.4",
    "@nestjs/core": "^7.5.4",
    "@nestjs/passport": "^7.1.3",
    "@nestjs/platform-socket.io": "^7.5.5",
    "@nestjs/platform-ws": "^7.5.5",
    "@nestjs/websockets": "^7.5.4",
    "@yarnpkg/pnpify": "^2.3.3",
    "body-parser": "^1.19.0",
    "cache-manager": "^3.4.0",
    "cache-manager-ioredis": "^2.1.0",
    "cache-manager-redis": "^0.6.0",
    "concat-stream": "^2.0.0",
    "debug": "^4.3.1",
    "deepmerge": "^4.2.2",
    "ioredis": "^4.19.2",
    "mongoose": "^5.10.15",
    "node-fetch": "^2.6.1",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "p-retry": "^4.2.0",
    "passport": "^0.4.1",
    "passport-shopify": "^0.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "shopify-admin-api": "^2.16.3",
    "shopify-token": "^4.0.0",
    "socket.io-redis": "^6.0.1",
    "typescript": "^4.0.5"
  },
  "devDependencies": {
    "@nestjs/testing": "^7.5.4",
    "@types/body-parser": "^1.19.0",
    "@types/cache-manager": "^2.10.3",
    "@types/cache-manager-ioredis": "^2.0.1",
    "@types/concat-stream": "^1.6.0",
    "@types/debug": "^4.1.5",
    "@types/deepmerge": "^2.2.0",
    "@types/elasticsearch": "^5.0.36",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/express-socket.io-session": "^1.3.4",
    "@types/ioredis": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/mongodb": "^3.5.33",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^14.14.8",
    "@types/node-fetch": "^2.5.7",
    "@types/p-queue": "^3.2.1",
    "@types/p-retry": "^3.0.1",
    "@types/passport": "^1.0.4",
    "@types/retry": "^0.12.0",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-redis": "^1.0.26",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0"
  },
  "resolutions": {
    "shopify-admin-api": "portal:../shopify-admin-api"
  }
}
